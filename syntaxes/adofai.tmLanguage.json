      }]
    },
    "constant": {
      "match": "\\b(?:true|false|null)\\b",
      "name": "constant.language.adofai"
    },
    "number": {
      "match": "-?(?:0|[1-9]\\d*)(?:\\.\\d+)?(?:[eE][+-]?\\d+)?",
      "name": "constant.numeric.adofai"
    },
    "string": {
      "begin": "\"",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.string.begin.adofai" }
      },
      "end": "\"",
      "endCaptures": {
        "0": { "name": "punctuation.definition.string.end.adofai" }
      },
      "name": "string.quoted.double.adofai",
      "patterns": [
        {
          "match": "\\\\(?:[\"\\\\/bfnrt]|u[0-9a-fA-F]{4})",
          "name": "constant.character.escape.adofai"
        },
        {
          "match": "\\\\.",
          "name": "invalid.illegal.unrecognized-string-escape.adofai"
        }
      ]
    },
    "array": {
      "begin": "\\[",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.array.begin.adofai" }
      },
      "end": "\\]",
      "endCaptures": {
        "0": { "name": "punctuation.definition.array.end.adofai" }
      },
      "name": "meta.structure.array.adofai",
      "patterns": [
        { "include": "#value" },
        {
          "match": ",",
          "name": "punctuation.separator.array.adofai"
        }
      ]
    },
    "object": {
      "begin": "\\{",
      "beginCaptures": {
        "0": { "name": "punctuation.definition.dictionary.begin.adofai" }
      },
      "end": "\\}",
      "endCaptures": {
        "0": { "name": "punctuation.definition.dictionary.end.adofai" }
      },
      "name": "meta.structure.dictionary.adofai",
      "patterns": [
        {
          "begin": "\"",
          "beginCaptures": {
            "0": { "name": "punctuation.definition.string.begin.adofai" }
          },
          "end": "\"",
          "endCaptures": {
            "0": { "name": "punctuation.definition.string.end.adofai" }
          },
          "name": "string.key.adofai",
          "patterns": [
            {
              "match": "\\\\(?:[\"\\\\/bfnrt]|u[0-9a-fA-F]{4})",
              "name": "constant.character.escape.adofai"
            }
          ]
        },
        {
          "begin": ":",
          "beginCaptures": {
            "0": { "name": "punctuation.separator.key-value.adofai" }
          },
          "end": "(?=,|\\})",
          "patterns": [{ "include": "#value" }]
        },
        {
          "match": ",",
          "name": "punctuation.separator.dictionary.adofai"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "match": "//.*$",
          "name": "comment.line.double-slash.adofai"
        },
        {
          "begin": "/\\*",
          "end": "\\*/",
          "name": "comment.block.adofai"
        }
      ]
    }
  }
}